{
  "name": "server",
  "version": "0.0.1",
  "main": "index.js",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/multer": "^1.4.5",
    "@types/node": "^15.12.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "gen-env-types": "^1.3.0",
    "nodemon": "^2.0.7",
    "ts-node": "10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "aws-sdk": "^2.922.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "multer": "^1.4.2",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.34",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "watch": "tsc -w",
    "dev": "cross-env NODE_ENV=development nodemon dist/index.js",
    "dev2": "ts-node src/index.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "tsc": "tsc --noEmit",
    "gen-env": "gen-env-types .env -o src/env.d.ts -e .",
    "gen:migration": "typeorm migration:generate -n <NAME>",
    "run:migration": "typeorm migration:run"
  }
}
